pipeline {
  agent any
  stages {
    stage('test') {
      steps {
        sh 'echo "this is test"'
      }
    }

    stage('development') {
      steps {
        sh 'echo "this is devlopment"'
      }
    }

    stage('production') {
      steps {
        sh 'echo "production"'
      }
    }

  }
}